      $request_to_view.fancybox({
        'href': $(this).attr("href"),
        'width': 710,
        'height': height,
        'onComplete': (function() {
          var collection;
          var tapuhi = true;
          var nlnzcat = false;

          if (tapuhi) {
            collection = 'TAPUHI';
          } else if (nlnzcat) {
            collection = 'NLNZcat';
          }

          if (collection) {
            _gaq.push(['_trackEvent', 'Request to view', collection, window.location.href]);
          }
        })
      });
    });
  </script>

