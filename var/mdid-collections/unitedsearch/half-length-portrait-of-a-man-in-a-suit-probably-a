        'ajax': {dataType: 'html'}
      });
      
      var $request_to_view = $("a#request-to-view")
      var height = parseInt($request_to_view.data("height"))
      
        _gaq.push(['_trackEvent','Page View', 'Landing Page', "collection: Harding, William James, 1826-1899 :Negatives of Wanganui district" ])
        _gaq.push(['_trackEvent','Page View', 'Landing Page', "collection: TAPUHI" ])
